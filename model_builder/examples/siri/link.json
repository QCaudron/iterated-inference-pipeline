{
  "name": "my_link",
  "description": "Over dispersed observation process",

  "observed": [{"id": "prev",
                "comment": "prevalence",
                "definition": ["I"],
                "time_series_id": ["city1__CDC__prev"]},
               {"id": "inc",
                "comment": "incidence",
                "definition": [{"from": "S", "to": "I"},{"from": "DU", "to": "I"}],
                "time_series_id": ["all__CDC__inc", "all__google__inc", "city2__CDC__inc"]}],

  "parameter": [{"id": "rep", "comment": "reporting rate"},
                {"id": "phi",  "comment": "over-dispertion"}],

  "model": {"distribution": "discretized_normal",
            "mean": "rep*prop*x",
            "var": "rep*(1.0-rep)*prop*x + (rep*phi*prop*x)**2"}
}
