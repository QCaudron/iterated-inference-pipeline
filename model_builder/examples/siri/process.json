{
  "name": "SIRI",
  "description": "SIRI model",

  "state": [{"id": "S", "comment": "Susceptible"},
            {"id": "I", "comment": "Infectious"}],

  "parameter": [{"id": "r0",  "comment": "basic reproduction number"},
                {"id": "v",   "comment": "recovery rate"},
                {"id": "sto", "comment": "noise"},
                {"id": "z",   "comment": "reinfection rate"},
                {"id": "mu",  "comment": "birth and death rate"}],

  "model": [{"from": "U",  "to": "S",  "rate": "mu*N", "comment":"birth"},
            {"from": "S",  "to": "I",  "rate": "noise__trans(sto)*r0/N*v*I", "tag":[{"id": "transmission", "by":["I"]}], "comment":"infection"},
            {"from": "I",  "to": "DU", "rate": "v", "comment":"recovery"},
            {"from": "DU", "to": "I",  "rate": "z*noise__trans(sto)*r0/N*v*I*(N-S-I)", "tag":[{"id": "transmission", "by":["I"]}], "comment":"re-infection"},
            {"from": "S",  "to": "U",  "rate": "mu", "comment":"death"},
            {"from": "I",  "to": "U",  "rate": "mu", "comment":"death"}],

  "pop_size_eq_sum_sv": false
}
