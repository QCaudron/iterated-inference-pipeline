{
 "name": "SAId",
 "description": "SI model with accute phase and diagnosed cases during Pre-AIDS phase",

 "state": [{"id": "S", "comment": "Susceptible"},
           {"id": "I", "comment": "Pre-AIDS phase"}],

 "parameter": [{"id": "beta",   "comment": "transmission rate for Pre-AIDS phase"},
	       {"id": "k",    "comment": "symptoms appearance rate"},
               {"id": "sto",  "comment": "noise intensity"},
	       {"id": "d",  "comment": "diagnostic rate"},
               {"id": "mu_b", "comment": "birth rate"},
               {"id": "mu_d", "comment": "death rate"}],

 "model": [{"from": "U", "to": "S",  "rate": "mu_b*N", "comment":"birth"},
           {"from": "S", "to": "I",  "rate": "beta*I/N", "tag": {"transmission": {"by":["I"]}}, "comment":"infection"},
           {"from": "I", "to": "DU", "rate": "correct_rate(k)+correct_rate(d)", "comment":"entering AIDS phase + diagnosis"},
	   {"from": "S", "to": "U",  "rate": "mu_d", "comment":"death"},
           {"from": "I", "to": "U",  "rate": "mu_d", "comment":"death"}],

 "white_noise": [
   {
     "reaction": [{"from":"S", "to": "I"}],
     "sd": "sto"
   }
 ],

 "pop_size_eq_sum_sv": false
}
