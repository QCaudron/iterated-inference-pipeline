{
  "name": "oBinomial",
  "description": "my comment",

  "observed": [
    {
      "id": "inc", "comment":"recorded cases",
      "definition": [{"from":"I", "to":"DU"}],
      "time_series_id": ["France__Sentinelles__inc"],
      "observation_id": "common"
    }
  ],

  "observation": [
    {
      "id": "common", 
      "parameter": [
        {"id": "rep", "comment": "reporting rate"},
        {"id": "phi",  "comment": "over-dispertion"}
      ],
      "model": {
        "distribution": "discretized_normal",
        "mean": "rep*prop*x",
        "var": "rep*(1.0-rep)*prop*x + (rep*phi*prop*x)**2"
      }
    }
  ]
}
